{
  "name": "showcase",
  "version": "0.1.0",
  "author": {
    "name": "Neekware Inc.",
    "url": "https://neekware.com"
  },
  "scripts": {
    "prettier": "node_modules/.bin/prettier",
    "pnpm turbo": "node_modules/.bin/turbo",
    "format": "prettier -w .",
    "prepare": "husky install",
    "build": "pnpm turbo run build",
    "clean": "pnpm turbo run clean",
    "dev": "pnpm turbo run dev --no-cache  --continue --concurrency=11",
    "start": "pnpm turbo run dev --no-cache  --continue --concurrency=11",
    "loc": "pnpm turbo run loc",
    "coverage": "pnpm turbo run coverage",
    "coveralls": "pnpm turbo run coveralls",
    "test": "pnpm turbo run test",
    "e2e": "pnpm turbo run e2e",
    "lint": "pnpm turbo run lint",
    "lint:prettier": "pnpm turbo run lint:prettier"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/node": "^20.3.1",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "turbo": "^1.10.6"
  },
  "packageManager": "pnpm@7.15.0"
}
