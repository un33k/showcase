{
  "name": "showcase",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Neekware Inc.",
    "url": "https://neekware.com"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "loc:update": "yarn ts-node -P ./tools/tsconfig.tools.json ./tools/loc.ts",
    "db:mig:dev": "prisma migrate dev --schema=prisma/schema.prisma",
    "db:mig:create": "prisma migrate dev --create-only --schema=prisma/schema.prisma",
    "db:mig:apply": "prisma migrate deploy --schema=prisma/schema.prisma",
    "db:reset": "prisma migrate reset --schema=prisma/schema.prisma",
    "db:studio": "prisma studio --schema=prisma/schema.prisma",
    "db:seed": "prisma db seed",
    "db:generate": "prisma generate",
    "postinstall": "pnpm db:generate && husky install"
  },
  "prisma": {
    "seed": "ts-node -P tools/tsconfig.tools.json prisma/seed.ts"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@mdi/js": "^7.2.96",
    "@mdi/react": "^1.6.1",
    "@prisma/client": "^4.15.0",
    "bcrypt": "^5.1.0",
    "next": "^13.4.4",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.8",
    "@types/react-dom": "18.2.4",
    "@types/uuid": "^9.0.1",
    "autoprefixer": "10.4.14",
    "commander": "^10.0.1",
    "daisyui": "^3.0.3",
    "eslint": "^8.42.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.12.1",
    "husky": "^8.0.3",
    "markdown-replace-section": "^0.4.0",
    "postcss": "8.4.24",
    "prisma": "^4.15.0",
    "prisma-dbml-generator": "^0.10.0",
    "tailwindcss": "3.3.2",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  }
}
